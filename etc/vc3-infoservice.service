[Unit]
Description=VC3 Information Service
After=network-online.target 
Wants=network-online.target

[Service]
EnvironmentFile=-/etc/sysconfig/vc3-info-service
ExecStart=/usr/libexec/vc3-info-service.init start
ExecStop=/usr/libexec/vc3-info-service.init stop
ExecReload=/usr/libexec/vc3-info-service.init restart


[Install]
WantedBy=multi-user.target
